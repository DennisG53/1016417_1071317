@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StreamGT - LogIn</title>
</head>
<body>
    <h1>StreamGT</h1>
    <h2>Hacer LogIn</h2>
    <div>
        <form method="post" style="margin: 5% 0 0">
            <div class="float-left">
                <p>Username</p>
                <p>Contraseña</p>
            </div>
            <div class="float-right">
                <input type="text" style="margin: 1px 0 2px" name="usernameinput" /><br />
                <input type="password" style="margin: 1px 0 2px" name="passwordinput" /><br />
                <input type="submit" value="Save" />
            </div>
        </form>
    </div>
</body>
</html>
